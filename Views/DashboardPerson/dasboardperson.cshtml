                <div class="thumbnail">
                    <form asp-action="GetVisita" asp-controller="Visitas">
                        <button type="submit" class="btn btn-info d-block mx-auto">
                            <img src="/img/visita.jpg" alt="Lights" style="width:100%">
                        </button>
                        <input type="text" name="txtUser" value="@ViewBag.Session" hidden />
                    </form>
                    <div class="text-center">
                        <h2 style=" color:darkblue">Visitas</h2>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
