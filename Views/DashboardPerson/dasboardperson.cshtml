                    <form asp-action="GetVehiculos" asp-controller="Vehiculos">
                        <button type="submit" class="btn btn-info d-block mx-auto">
                            <img src="/img/vehiculo.png" alt="Nature" style="width:100%">
                        </button>
                    </form>
                    <div class="text-center">
                        <h2 style=" color:darkblue">Vehiculos</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
</html>

